	"scripts": {
		"start": "react-scripts start",
		"build": "npx browserslist@latest --update-db && react-scripts build",
		"test": "DEBUG_PRINT_LIMIT=9999999 react-scripts test --silent=false --verbose=false",
		"eject": "react-scripts eject",
		"checkTs": "tsc --noEmit",
		"format": "prettier --write .",
		"lint": "eslint --max-warnings 0 --ext .ts --ext .tsx --ext .js --ext .jsx src/",
		"lint:fix": "eslint --max-warnings 0 --fix --ext .ts --ext .tsx --ext .js --ext .jsx src/",
		"precommit": "npm run format && npm run lint:fix && npm run checkTs",
		"prepare": "husky install"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@testing-library/dom": "^7.23.0",
		"@testing-library/jest-dom": "^5.11.5",
		"@testing-library/react": "^10.4.9",
		"@testing-library/react-hooks": "^3.7.0",
